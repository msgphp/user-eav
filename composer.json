{
    "name": "msgphp/user-eav",
    "type": "library",
    "description": "A bridge between the `User` and `EAV` domain",
    "keywords": [
        "ddd",
        "domain-driven-design",
        "rich domain model",
        "user",
        "entity-attribute-value",
        "eav"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "require": {
        "msgphp/eav": "^0.13",
        "msgphp/user": "^0.13"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.13-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\User\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
