{
    "name": "msgphp/user-eav",
    "type": "library",
    "license": "MIT",
    "description": "A bridge between the EAV and user domain",
    "keywords": ["ddd", "domain-driven-design", "rich domain model", "user", "entity-attribute-value", "eav"],
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MsgPhp\\User\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "msgphp/eav": "^0.8",
        "msgphp/user": "^0.8"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "symfony/phpunit-bridge": "^3.4.9|^4.0.9",
        "symfony/var-dumper": "^3.4|^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
